{
  "name": "serverless-es-logs",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A Serverless plugin to transport logs to ElasticSearch",
  "author": "Daniel Cottone",
  "engines": {
    "node": ">=6.10.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "compile:clean": "rimraf dist",
    "compile:watch": "tsc -w",
    "lint": "tslint --project . --format stylish",
    "prepublish": "yarn compile:clean && yarn compile",
    "release": "semantic-release",
    "test": "echo 'not yet!'"
  },
  "release": {
    "debug": true,
    "noCi": true,
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.0",
    "@semantic-release/git": "7.0.1",
    "@semantic-release/github": "5.0.1",
    "@types/lodash": "4.14.115",
    "rimraf": "2.6.2",
    "semantic-release": "15.8.1",
    "tslint": "5.11.0",
    "typescript": "2.9.2"
  }
}
